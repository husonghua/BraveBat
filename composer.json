{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "bugsnag/bugsnag-laravel": "^2.0",
    "calebporzio/sushi": "^2.0",
    "doctrine/dbal": "^3.0",
    "fabpot/goutte": "^4.0",
    "fakerphp/faker": "^1.13",
    "fideloper/proxy": "^4.2",
    "fruitcake/laravel-cors": "^2.0",
    "google/protobuf": "^3.15",
    "guzzlehttp/guzzle": "^7.3",
    "http-interop/http-factory-guzzle": "^1.0",
    "j7mbo/twitter-api-php": "^1.0",
    "laravel/framework": "^8.0",
    "laravel/horizon": "^5.0",
    "laravel/sanctum": "^2.2",
    "laravel/slack-notification-channel": "^2.0",
    "laravel/telescope": "4.4.6",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^3.2",
    "league/flysystem-aws-s3-v3": "^1.0",
    "m1guelpf/laravel-fun": "^1.2",
    "maxbanton/cwh": "^2.0",
    "meilisearch/meilisearch-php": "^0.17.0",
    "nesk/puphpeteer": "^2.0",
    "paquettg/php-html-parser": "^3.0",
    "predis/predis": "^1.1",
    "protobuf-php/protobuf": "^0.1.3",
    "sammyjo20/lasso": "^1.2",
    "silviolleite/laravelpwa": "~2.0",
    "spatie/browsershot": "^3.35",
    "spatie/laravel-schedule-monitor": "^2.2",
    "spatie/laravel-sitemap": "^6.0",
    "zircote/swagger-php": "^3.1"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.3",
    "brianium/paratest": "^6.1",
    "facade/ignition": "^2.0",
    "mockery/mockery": "^1.3.1",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "Publishers_pb\\": "app/Services/Publishers_pb",
      "GPBMetadata\\": "app/Services/GPBMetadata",
      "Foo\\Bar\\": "app/Services/Foo/Bar"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan horizon:publish --ansi",
      "@php artisan telescope:publish --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
